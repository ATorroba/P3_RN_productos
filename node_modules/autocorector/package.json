{
  "name": "autocorector",
  "version": "3.1.3",
  "description": "autoCOREctor de asignaturas y MOOCs de la ETSIT-UPM",
  "main": "scripts/launcher.js",
  "disableHashCheck": true,
  "devLogs": true,
  "capturesFolder": "miscapturas",
  "capturesNumber": 2,
  "scripts": {
    "checks": "node scripts/launcher.js --debug",
    "upload": "node scripts/launcher.js --debug --upload",
    "log": "node scripts/launcher.js --saveLog salida2.log",
    "uglify": "webpack",
    "buildold": "webpack && cd dist && pkg ../scripts/launcher.min.js && mv launcher.min-linux corrector-linux && mv launcher.min-win.exe corrector-windows.exe && mv launcher.min-macos corrector-macos",
    "build:dev": "webpack --mode development && cd dist && pkg ../scripts/launcher.min.js && mv launcher.min-linux corrector-linux && mv launcher.min-win.exe corrector-windows.exe && mv launcher.min-macos corrector-macos",
    "lint": "eslint . ",
    "prepublishold": "npm run build"
  },
  "pkg": {
    "scripts": "scripts/launcher.js",
    "assets": "scripts/jest/*",
    "targets": [
      "node16-win-x64"
    ],
    "outputPath": "dist"
  },
  "bin": {
    "autocorector": "scripts/launcher.js"
  },
  "keywords": [
    "MOOCs",
    "self-assessment"
  ],
  "author": "ging",
  "license": "GNU",
  "info": {
    "githubURL": "IWEB-UPM/P1_el_tiempo",
    "serverUrl": "https://autocorector.dit.upm.es/",
    "courseId": "8249",
    "assignmentId": "73146"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ging/autoCOREctor",
    "directory": "client"
  },
  "engines": {
    "node": ">=16"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.6",
    "@babel/runtime": "^7.18.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "archiver": "5.3.1",
    "babel-jest": "^29.0.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "chai": "4.3.6",
    "chalk": "4.1.2",
    "eslint": "^8.18.0",
    "figlet": "1.5.2",
    "global-jsdom": "^8.5.0",
    "jest": "^29.0.1",
    "jest-cli": "^29.0.1",
    "jest-environment-jsdom": "^29.0.1",
    "jsdom": "^20.0.0",
    "mocha": "^10.0.0",
    "mongoose": "^6.5.2",
    "needle": "^3.1.0",
    "object-hash": "^3.0.0",
    "pkg": "^5.8.0",
    "react-app-polyfill": "^3.0.0",
    "react-router-dom": "^6.3.0",
    "react-test-renderer": "^18.2.0",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0",
    "zombie": "^6.1.4"
  }
}
